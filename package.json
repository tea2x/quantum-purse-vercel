    "@types/chai": "^5.2.0",
    "@types/mocha": "^10.0.10",
    "@types/react": "^18.2.64",
    "@types/react-dom": "^18.2.21",
    "@types/sinon": "^17.0.4",
    "babel-loader": "^10.0.0",
    "chai": "^5.2.0",
    "compression-webpack-plugin": "^7.1.2",

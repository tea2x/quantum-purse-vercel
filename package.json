    "gh-pages": "^6.3.0",
    "html-webpack-plugin": "^5.3.1",
    "karma": "^6.4.4",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^3.2.0",
    "karma-mocha": "^2.0.1",
    "karma-typescript": "^5.5.4",
    "karma-webpack": "^5.0.1",
    "mocha": "^11.1.0",
